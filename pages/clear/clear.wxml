<view class="page">
    <view class="page__bd">
        <view class="weui-cells__title">本地数据</view>
        <view class="weui-cells weui-cells_after-title">
            <view id="cache" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="clear">
                <view class="weui-cell__bd">清除本地缓存</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
        </view>
        <view class="weui-cells__title">服务器端数据</view>
        <view class="weui-cells weui-cells_after-title">
            <view id="dormitory" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="clear">
                <view class="weui-cell__bd">清除寝室信息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view id="card" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="clear">
                <view class="weui-cell__bd">清除一卡通绑定信息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view id="library" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="clear">
                <view class="weui-cell__bd">清除图书馆绑定信息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            <view id="account" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="clear">
                <view class="weui-cell__bd">删除账号</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
        </view>

    </view>
</view>